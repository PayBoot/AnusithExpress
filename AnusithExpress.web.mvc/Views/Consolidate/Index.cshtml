
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
@model List<AnousithExpress.Data.SingleViewModels.CustomerNotification>
<h2>Index</h2>

@foreach (var customer in Model)
{
<div>
    <a href="@Url.Action("ItemAlreadySent","Consolidate", new { CustId = customer.CustomerId})">
        <input type="button" name="customer" value="@customer.CustomerId" /><span>@customer.NumberOfItem</span>
    </a>    
</div>

}


